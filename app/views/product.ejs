<%- include('partials/header') %>
	<!--Contenu de la page-->
	<main>
		<section class="product_page">
			<img src="/public/images/coffees/<%=searchedCoffee.ref%>.png" alt="image_produit">
			<article class="product_info">
			<time datetime="2018-07-07"">Publié le <%= date %></time>
				<h2><%=searchedCoffee.name%></h2>
				<p><%=searchedCoffee.text%></p>
				<p><span>Origine :</span> <%=searchedCoffee.origin%> </p>
				<p><span>Charactéristique principale :</span> <%=searchedCoffee.caracteristic%></p>
				<p><span>Prix au Kg : </span><%=searchedCoffee.price_kg%> </p>
				<button class="button_main"> <a href="/catalogue">Catalogue</a></button>
			</article>
		</section>
	</main>
	
	<!--Pied de page-->
<%- include('partials/footer') %>